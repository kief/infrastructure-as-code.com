{
  "Statement":
  [
    {
      "Effect":"Allow",
      "Principal": 
      {
        "AWS": [ "arn:aws:iam::637221145048:user/infrasite" ]
      },
      "Action": 
      [
        "s3:AbortMultipartUpload",
        "s3:DeleteObject",
        "s3:DeleteObjectVersion",
        "s3:GetObject",
        "s3:GetObjectAcl",
        "s3:GetObjectVersion",
        "s3:GetObjectVersionAcl",
        "s3:PutObject",
        "s3:PutObjectAcl"
      ],
      "Resource": "arn:aws:s3:::infrastructure-as-code.com/*"
    },
    {
      "Effect":"Allow",
      "Principal": 
      {
        "AWS": [ "arn:aws:iam::637221145048:user/infrasite" ]
      },
      "Action": 
      [
        "s3:GetBucketLocation",
        "s3:ListBucket",
        "s3:ListBucketMultipartUploads"
      ],
      "Resource": "arn:aws:s3:::infrastructure-as-code.com"
    }
  ]
}
